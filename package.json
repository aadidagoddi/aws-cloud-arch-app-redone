{
  "name": "aws-cloud-arch-project",
  "version": "1.0.0",
  "description": "AI-powered AWS cloud architecture visualization tool",
  "main": "electron/main.js",
  "scripts": {
    "test": "echo \"Error: no tsst specified\" && exit 1",
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vite build && electron-builder"
  },
  "keywords": [
    "aws",
    "cloud",
    "architecture",
    "ai",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "aws-sdk": "^2.1693.0",
    "framer-motion": "^12.24.11",
    "lucide-react": "^0.562.0",
    "monaco-editor": "^0.55.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0",
    "three": "^0.182.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.awscloudarchitect.app",
    "productName": "AWS Cloud Architect",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ]
    }
  }
}
